{
  "name"        : "consigliere/streams",
  "description" : "Streams description.",
  "keywords"    : [
    "consigliere",
    "streams"
  ],
  "license"     : "MIT",
  "homepage"    : "https://github.com/consigliere/streams",
  "support"     : {
    "issues": "https://github.com/consigliere/streams/issues",
    "source": "https://github.com/consigliere/streams"
  },
  "authors"     : [
    {
      "name" : "anonymoussc",
      "email": "bcd3a161@opayq.com"
    }
  ],
  "require"     : {
    "php"                      : "~5.6|~7.0",
    "squizlabs/php_codesniffer": "^2.3",
    "illuminate/support"       : "5.3.*",
    "intervention/image"       : "^2.3",
    "doctrine/dbal"            : "^2.5"
  },
  "require-dev" : {
    "phpunit/phpcov"     : "~3.0",
    "phpunit/phpunit"    : "~5.0",
    "laravel/framework"  : "~5.3.8",
    "orchestra/testbench": "~3.3.0"
  },
  "autoload"    : {
    "psr-4": {
      "RAD\\Streams\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RAD\\Streams\\Tests\\": "tests/"
    }
  },
  "scripts"     : {
    "test"       : "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style"  : "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra"       : {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config"      : {
    "sort-packages": true
  }
}
